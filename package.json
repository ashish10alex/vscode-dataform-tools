{
  "name": "dataform-lsp-vscode",
  "displayName": "Dataform tools",
  "publisher": "ashishalex",
  "icon": "media/images/dtools.png",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashish10alex/dataform-lsp-vscode.git"
  },
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*.sqlx",
    "onCommand:extension.enable",
    "onCommand:extension.disable"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dataform-lsp-vscode.enable",
        "category": "Dataform",
        "title": "enable lsp"
      },
      {
        "command": "dataform-lsp-vscode.disable",
        "category": "Dataform",
        "title": "disable lsp"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
